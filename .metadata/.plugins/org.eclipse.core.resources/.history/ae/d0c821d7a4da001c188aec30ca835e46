package com.qa.calculator;

public class Calculator {

	public static void main(String[] args) {
		int num1 = 10;
		int num2 = 3;
		Operators myNum = new Operators(num1, num2);
		
		int physics = 124;
		int chemistry = 111;
		int biology = 117;
		ExamResults MyScore = new ExamResults(physics, chemistry, biology);
		
		
		System.out.println(myNum.AddTwoNum());
		System.out.println(myNum.MultiplyTwoNum());
		System.out.println(myNum.SubtractTwoNum());
		System.out.println(myNum.DivideTwoNum());
		System.out.println(MyScore.YourResults());
		System.out.println(MyScore.YourPercentage());
	}

}
